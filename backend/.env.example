# API Keys (Required)
GROQ_API_KEY=your_groq_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Optional: YouTube API for enhanced metadata
YOUTUBE_API_KEY=your_youtube_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Optional: Webshare proxy for enhanced download reliability
WEBSHARE_USERNAME=your_webshare_username
WEBSHARE_PASSWORD=your_webshare_password

# Server Configuration
DEBUG=true
HOST=0.0.0.0
PORT=8000

# CORS Configuration (for development)
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

# File Storage
TEMP_DIR=/tmp/ytfetch
MAX_FILE_SIZE_MB=100

# Rate Limiting
MAX_CONCURRENT_TRANSCRIPTIONS=3
GROQ_RPM_LIMIT=100

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs (from your Stripe dashboard)
STRIPE_PRICE_FREE=price_free_id
STRIPE_PRICE_PRO=price_pro_id
STRIPE_PRICE_ENTERPRISE=price_enterprise_id

# Optional: Stripe Product IDs for reference
STRIPE_PRODUCT_FREE=prod_free_id
STRIPE_PRODUCT_PRO=prod_pro_id
STRIPE_PRODUCT_ENTERPRISE=prod_enterprise_id

# Stripe URLs (optional, defaults provided)
STRIPE_SUCCESS_URL=http://localhost:3000/billing?success=true&session_id={CHECKOUT_SESSION_ID}
STRIPE_CANCEL_URL=http://localhost:3000/billing?canceled=true
STRIPE_RETURN_URL=http://localhost:3000/billing